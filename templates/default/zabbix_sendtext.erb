#!/bin/bash
# Generated by Chef

TWILIO_ID=<%= node.zabbix.twilio[:id] %>
TWILIO_TOKEN=<%= node.zabbix.twilio.token %>
TWILIO_PHONE=<%= node.zabbix.twilio.phone %>

curl -X POST "https://api.twilio.com/2010-04-01/Accounts/${TWILIO_ID}/SMS/Messages.json" -u ${TWILIO_ID}:${TWILIO_TOKEN} -d "From=%2B${TWILIO_PHONE}" -d "To=${1}" -d "Body=${2}"
